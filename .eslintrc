{
  "parser": "babel-eslint",
  "extends": "react",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react-hooks"],
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": ["error", "only-multiline"],
    "react-hooks/rules-of-hooks": "error"
  }
}
